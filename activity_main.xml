<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/fridge_bottom"
        android:layout_width="359dp"
        android:layout_height="302dp"
        android:layout_marginStart="35dp"

        android:layout_marginEnd="25dp"
        android:layout_marginBottom="144dp"
        android:src="@drawable/fridge_bottom_close"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fridge_top" />

    <ImageView
        android:id="@+id/fridge_top"
        android:layout_width="305dp"
        android:layout_height="176dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="176dp"
        android:layout_marginEnd="49dp"
        app:layout_constraintBottom_toTopOf="@+id/fridge_bottom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fridge_top_close" />

    <ImageButton
        android:id="@+id/meat"
        android:layout_width="70dp"
        android:layout_height="43dp"
        android:layout_marginStart="59dp"
        android:layout_marginBottom="16dp"
        android:adjustViewBounds="true"
        android:background="#00ffffff"
        android:scaleType="fitCenter"
        android:src="@drawable/meat"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/fridge_bottom"
        app:layout_constraintEnd_toEndOf="@+id/fridge_bottom"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="@+id/fridge_bottom"
        app:layout_constraintTop_toTopOf="@+id/fridge_bottom"
        app:layout_constraintVertical_bias="0.599" />

    <ImageButton
        android:id="@+id/egg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="47dp"
        android:adjustViewBounds="true"
        android:background="#00ffffff"
        android:scaleType="fitCenter"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/fridge_bottom"
        app:layout_constraintEnd_toEndOf="@+id/fridge_bottom"
        app:layout_constraintHorizontal_bias="0.474"
        app:layout_constraintStart_toStartOf="@+id/fridge_bottom"
        app:layout_constraintTop_toTopOf="@+id/fridge_bottom"
        app:layout_constraintVertical_bias="0.728"
        app:srcCompat="@drawable/egg" />

    <ImageButton
        android:id="@+id/kimchi"
        android:layout_width="73dp"
        android:layout_height="72dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="14dp"
        android:adjustViewBounds="true"
        android:background="#00ffffff"
        android:scaleType="fitCenter"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/fridge_bottom"
        app:layout_constraintEnd_toEndOf="@+id/fridge_bottom"
        app:layout_constraintHorizontal_bias="0.147"
        app:layout_constraintStart_toStartOf="@+id/fridge_bottom"
        app:layout_constraintTop_toTopOf="@+id/fridge_bottom"
        app:layout_constraintVertical_bias="0.122"
        app:srcCompat="@drawable/kimchi" />

    <ImageButton
        android:id="@+id/ice"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:layout_marginStart="62dp"
        android:layout_marginBottom="51dp"
        android:adjustViewBounds="true"
        android:background="#00ffffff"
        android:scaleType="fitCenter"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/fridge_bottom"
        app:layout_constraintEnd_toEndOf="@+id/fridge_top"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="@+id/fridge_top"
        app:layout_constraintTop_toTopOf="@+id/fridge_top"
        app:layout_constraintVertical_bias="0.625"
        app:srcCompat="@drawable/ice" />

    <ImageButton
        android:id="@+id/dumpling"
        android:layout_width="66dp"
        android:layout_height="58dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="18dp"
        android:adjustViewBounds="true"
        android:background="#00ffffff"
        android:scaleType="fitCenter"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/fridge_top"
        app:layout_constraintEnd_toEndOf="@+id/fridge_top"
        app:layout_constraintHorizontal_bias="0.187"
        app:layout_constraintStart_toStartOf="@+id/fridge_top"
        app:layout_constraintTop_toTopOf="@+id/fridge_top"
        app:layout_constraintVertical_bias="0.4"
        app:srcCompat="@drawable/dumpling" /><![CDATA[



    "
        app:layout_constraintBottom_toBottomOf="@+id/fridge_bottom"
        app:layout_constraintEnd_toEndOf="@+id/fridge_bottom"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/fridge_bottom"
        app:layout_constraintTop_toTopOf="@+id/fridge_bottom"
        app:layout_constraintVertical_bias="0.76"
        app:srcCompat="@drawable/meet" />


]]>
</androidx.constraintlayout.widget.ConstraintLayout>
